{
  "expo": {
    "name": "BeHappy",
    "slug": "BeHappy",
    "platforms": ["android"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon_green.png",
    "splash": {
      "image": "./assets/behappy.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFFFFF"
    },
    "updates": {
